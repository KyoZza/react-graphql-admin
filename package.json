{
  "name": "react-graphql-admin",
  "version": "0.0.1",
  "description": "admin panel build with react.js and GraphQL",
  "main": "index.js",
  "scripts": {
    "start": "REACT_APP_API_URL=foo react-scripts start",
    "start:win": "set REACT_APP_API_URL=bar && react-scripts start",
    "apollo:introspect": "node ./introspectApolloServer.js",
    "build": "yarn run apollo:introspect && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "admin",
    "react",
    "apollo",
    "responsive"
  ],
  "author": {
    "name": "Sandro Scheffmann"
  },
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@types/react": "^16.9.17",
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-timeout": "^4.0.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "node-fetch": "^2.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.3",
    "uuid": "^3.4.0"
  }
}
